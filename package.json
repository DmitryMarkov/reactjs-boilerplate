{
  "name": "reactjs-boilerplate",
  "appName": "HelloWorld",
  "version": "0.9.0",
  "description": "ReactJS Boilerplate",
  "main": "./lib/HelloWorld.min.js",
  "repository": "git@github.com:crazyjuice/reactjs-boilerplate.git",
  "author": "Piotr Sikora",
  "license": "MIT",
  "scripts": {
    "precommit": "lint-staged",
    "test": "mocha \"src/**/*.test.js*\" -r babel-core/register -r ignore-styles -r setupTests.js",
    "test:cover": "nyc npm test",
    "test:watch": "npm run test -- --watch --watch-extensions js",
    "coveralls": "npm run test:cover && cat ./coverage/lcov.info | coveralls",
    "lint": "eslint --ext .js src && npm run lint:post",
    "lint:fix": "eslint --fix --ext .js src && npm run lint:post",
    "lint:post": "echo \"No ESLint errors!!!\"",
    "clean": "rimraf dist lib package .nyc_output coverage *.tgz webpack-stats* yarn-error*",
    "analyze": "webpack-bundle-analyzer",
    "pre:build": "npm run lint && cross-env NODE_ENV=production webpack --color --progress --config webpack.config.prod.js --json > webpack-stats.json",
    "build": "npm run pre:build -- --env.ANALYZE=0 --env.SOURCE_MAP=0",
    "build:analyze": "npm run pre:build -- --env.ANALYZE=1",
    "build:map": "npm run pre:build -- --env.ANALYZE=0 --env.SOURCE_MAP=1",
    "lib": "npm run lint && cross-env NODE_ENV=production webpack --color --progress --config webpack.config.lib.js --json > webpack-stats.json",
    "build:lib": "rimraf lib && npm run lib -- --env.MINIFY=1 --env.SOURCE_MAP=0",
    "build:lib:analyze": "npm run build:lib -- --env.ANALYZE=1",
    "build:lib:map": "rimraf lib && npm run lib -- --env.MINIFY=1 --env.SOURCE_MAP=1",
    "server": "node prodServer.js",
    "server:dev": "node devServer.js",
    "serve": "npm run lint && webpack-dev-server --open --color --progress --config webpack.config.dev.js --env.MIDDLEWARE=0",
    "start": "npm run lint && npm run server:dev",
    "start:analyze": "npm run lint && npm run server:dev --env.ANALYZE=1",
    "start:dev": "npm run serve",
    "start:dev:analyze": "npm run serve -- --env.ANALYZE=1"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix --ext .js src",
      "git add"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.1.0",
    "axios": "^0.18.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.14",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^0.1.19",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "enzyme": "^3.4.1",
    "enzyme-adapter-react-16": "^1.2.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-html": "^4.0.5",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.10.0",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.12.0",
    "lint-staged": "^7.2.0",
    "lodash": "^4.17.10",
    "mini-css-extract-plugin": "^0.4.1",
    "mocha": "^5.2.0",
    "moment": "^2.22.2",
    "morgan": "^1.9.0",
    "node-sass": "^4.9.3",
    "normalize.css": "^8.0.0",
    "nyc": "^12.0.2",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.14.2",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-hot-loader": "^4.3.4",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-test-renderer": "^16.4.2",
    "redux": "^4.0.0",
    "redux-devtools": "^3.4.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "script-loader": "^0.7.2",
    "sinon": "^6.1.5",
    "style-loader": "^0.22.1",
    "stylelint": "^9.4.0",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.5",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-dev-server": "^3.1.5",
    "webpack-hot-middleware": "^2.22.3"
  },
  "dependencies": {}
}
