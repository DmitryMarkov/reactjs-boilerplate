{
  "name": "reactjs-sandbox",
  "version": "0.4.7",
  "description": "ReactJS Sandbox",
  "main": "./src/app.jsx",
  "repository": "git@github.com:crazyjuice/reactjs-sandbox.git",
  "author": "Piotr Sikora",
  "license": "MIT",
  "scripts": {
    "test": "mocha --require babel-core/register --require ignore-styles src/**/*.test.jsx",
    "lint": "eslint --ext .jsx,.js src && npm run lint:post",
    "lint:fix": "eslint --fix --ext .jsx,.js src && npm run lint:post",
    "lint:post": "echo \"No ESLint errors!!!\"",
    "clean": "rimraf dist",
    "build": "npm run lint && webpack --color --progress --config webpack.config.prod.js --json > webpack-stats.json",
    "server:dev": "node server.js",
    "server": "node index.js",
    "serve": "webpack-dev-server --open --color --progress --config webpack.config.dev.js --env.NO_MIDDLEWARE",
    "start:dev": "npm run lint && npm run serve",
    "start": "npm run lint && npm run server:dev"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^0.1.17",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "enzyme": "^3.2.0",
    "eslint": "^4.13.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-html": "^4.0.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "favicons-webpack-plugin": "^0.0.7",
    "file-loader": "^1.1.5",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "ignore-styles": "^5.0.1",
    "mocha": "^4.0.1",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.0.9",
    "prop-types": "^15.6.0",
    "react-test-renderer": "^16.2.0",
    "redux-devtools": "^3.4.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "script-loader": "^0.7.2",
    "sinon": "^4.1.3",
    "style-loader": "^0.19.1",
    "stylelint": "^8.4.0",
    "uglifyjs-webpack-plugin": "^1.1.4",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.1",
    "webpack-dev-server": "^2.9.7",
    "webpack-hot-middleware": "^2.21.0"
  },
  "dependencies": {
    "autoprefixer": "^7.2.3",
    "babel-polyfill": "^6.26.0",
    "express": "^4.16.2",
    "lodash": "^4.17.4",
    "moment": "^2.20.1",
    "morgan": "^1.9.0",
    "normalize.css": "^7.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^3.1.3",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2"
  }
}
